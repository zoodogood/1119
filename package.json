  "main": "index.js",
  "scripts": {
    "start": "node ./index.js",
    "watch-runtime": "node --watch ./index.js",
    "build": "node ./folder/scripts/build.bat.js",
    "dev": "rollup -c ./src/site/rollup.config.mjs -w",
    "site-build": "rollup -c ./src/site/rollup.config.mjs",
    "site-watch": "sirv static --no-clear",
    "createPagesExports": "node ./src/site/scripts/createPagesExports.js",
