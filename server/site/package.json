    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "sass": "^1.57.1",
    "svelte": "^3.0.0",
    "svelte-preprocess": "^5.0.0"
  },
  "dependencies": {
    "@discordjs/collection": "^1.3.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@zoodogood/utils": "^0.7.4",
    "sirv-cli": "^2.0.0",
    "svelte-markdown": "^0.2.3"
